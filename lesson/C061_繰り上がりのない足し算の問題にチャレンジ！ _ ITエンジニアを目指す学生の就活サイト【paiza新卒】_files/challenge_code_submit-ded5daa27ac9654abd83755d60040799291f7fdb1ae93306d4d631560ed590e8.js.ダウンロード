window.compile_ang_test=function(e){if($("#language_id").val())if(window.editor.getSession().getValue()){$(document).trigger("compile_and_test:begin");var i=$(".output_wrap"),t=$(".loadingBox"),o=$("#do_compile");i.hide(),t.fadeIn(),o.css("visibility","hidden");var a={code:window.editor.getSession().getValue(),language_id:$("#language_id").val(),sample_input_no:$("#sample_input_no").val()};$.ajax({url:e,type:"POST",dataType:"json",data:a,timeout:6e4}).done(function(e){o.css("visibility","visible"),t.hide(),i.show(),window.ace_editor_annotate_output(window.editor,e.result.annotations),$("#comp_test").removeAttr("disabled"),$("#code_submit").removeAttr("disabled"),i.html(e.html),$(document).trigger("compile_and_test:complete",{result:e.result})}).fail(function(e,a){o.css("visibility","visible"),t.hide(),i.show(),$("#comp_test").removeAttr("disabled"),$("#code_submit").removeAttr("disabled"),msg="timeout"===a?"\u63d0\u51fa\u524d\u52d5\u4f5c\u78ba\u8a8d\u306e\u5b9f\u884c\u304c\u30bf\u30a4\u30e0\u30a2\u30a6\u30c8\u3057\u307e\u3057\u305f\u3002\n\u30b8\u30e3\u30c3\u30b8\u30b5\u30fc\u30d0\u304c\u6df7\u96d1\u3057\u3066\u3044\u308b\u305f\u3081\u5c11\u3057\u6642\u9593\u3092\u7f6e\u3044\u305f\u5f8c\u3001\u5b9f\u884c\u3057\u3066\u304f\u3060\u3055\u3044\u3002":"\u63d0\u51fa\u524d\u52d5\u4f5c\u78ba\u8a8d\u304c\u5b9f\u884c\u3067\u304d\u307e\u305b\u3093\u3067\u3057\u305f\u3002\u304a\u624b\u6570\u3067\u3059\u304c\u5c11\u3057\u6642\u9593\u3092\u7f6e\u3044\u305f\u5f8c\u3001\u5b9f\u884c\u3057\u3066\u304f\u3060\u3055\u3044",alert(msg)})}else alert("\u30b3\u30fc\u30c9\u3092\u5165\u529b\u3057\u3066\u304f\u3060\u3055\u3044");else alert("\u8a00\u8a9e\u3092\u5165\u529b\u3057\u3066\u304f\u3060\u3055\u3044")},function(){var e=!1;window.hand_in_code=function(i){if(!e)if(void 0===i&&(i="\u4e00\u5ea6\u63d0\u51fa\u3059\u308b\u3068\u4fee\u6b63\u3067\u304d\u307e\u305b\u3093\u3002\u30b3\u30fc\u30c9\u3092\u63d0\u51fa\u3057\u3066\u3082\u3088\u308d\u3057\u3044\u3067\u3059\u304b\uff1f"),$("#language_id").